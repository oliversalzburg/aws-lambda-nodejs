{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "yarn3-typescript-nodejs-esbuild",
  "license": "MIT",
  "author": "Oliver Salzburg <oliver.salzburg@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "yarn run build:tsc",
    "build:esbuild": "node build.js",
    "build:tsc": "tsc",
    "clean": "rm -rf ./output",
    "lint": "yarn run lint:all",
    "lint:all": "yarn run lint:eslint && yarn run lint:prettier && yarn run lint:tsc",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "test": "yarn build && .scripts/test.sh"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "2.3.0",
    "@aws-lambda-powertools/metrics": "2.3.0",
    "@aws-lambda-powertools/tracer": "2.3.0",
    "@aws-sdk/client-secrets-manager": "3.606.0",
    "@middy/core": "5.4.2",
    "@middy/event-normalizer": "5.4.2",
    "@middy/http-cors": "5.4.2",
    "@middy/http-error-handler": "5.4.2",
    "@middy/http-event-normalizer": "5.4.2",
    "@middy/http-header-normalizer": "5.4.2",
    "@middy/http-json-body-parser": "5.4.2",
    "@middy/http-response-serializer": "5.4.2",
    "@middy/http-security-headers": "5.4.2",
    "@middy/secrets-manager": "5.4.2",
    "@middy/validator": "5.4.2",
    "@oliversalzburg/js-utils": "0.0.57",
    "aws-lambda": "1.0.7",
    "json-schema-to-ts": "3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "9.6.0",
    "@oliversalzburg/eslint-config": "0.0.13",
    "@types/aws-lambda": "8.10.140",
    "@types/chai": "4.3.16",
    "@types/mocha": "10.0.7",
    "@types/node": "20.14.9",
    "chai": "5.1.1",
    "esbuild": "0.21.5",
    "eslint": "9.6.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "48.5.0",
    "eslint-plugin-tsdoc": "0.3.0",
    "globals": "15.6.0",
    "lint-staged": "15.2.7",
    "mocha": "10.5.2",
    "prettier": "3.3.2",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-sh": "0.14.0",
    "typescript": "5.5.2",
    "typescript-eslint": "8.0.0-alpha.34"
  },
  "engines": {
    "node": "20"
  },
  "packageManager": "yarn@4.3.1"
}
