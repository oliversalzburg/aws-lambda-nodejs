{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "yarn3-typescript-nodejs-esbuild",
  "license": "MIT",
  "author": "Oliver Salzburg <oliver.salzburg@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "yarn run build:tsc",
    "build:esbuild": "node build.js",
    "build:tsc": "tsc",
    "clean": "rm -rf ./output",
    "lint": "yarn run lint:all",
    "lint:all": "yarn run lint:eslint && yarn run lint:prettier && yarn run lint:tsc",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "test": "yarn build && .scripts/test.sh"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "2.8.0",
    "@aws-lambda-powertools/metrics": "2.8.0",
    "@aws-lambda-powertools/tracer": "2.8.0",
    "@aws-sdk/client-secrets-manager": "3.654.0",
    "@middy/core": "5.5.0",
    "@middy/event-normalizer": "5.5.0",
    "@middy/http-cors": "5.5.0",
    "@middy/http-error-handler": "5.5.0",
    "@middy/http-event-normalizer": "5.5.0",
    "@middy/http-header-normalizer": "5.5.0",
    "@middy/http-json-body-parser": "5.5.0",
    "@middy/http-response-serializer": "5.5.0",
    "@middy/http-security-headers": "5.5.0",
    "@middy/secrets-manager": "5.5.0",
    "@middy/validator": "5.5.0",
    "@oliversalzburg/js-utils": "0.1.1",
    "@types/json-schema": "7.0.15",
    "aws-lambda": "1.0.7",
    "json-schema-to-ts": "3.1.1"
  },
  "devDependencies": {
    "@eslint/js": "9.11.1",
    "@oliversalzburg/eslint-config": "1.1.0",
    "@types/aws-lambda": "8.10.145",
    "@types/chai": "4.3.20",
    "@types/mocha": "10.0.8",
    "@types/node": "20.16.6",
    "chai": "5.1.1",
    "esbuild": "0.24.0",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "50.2.4",
    "eslint-plugin-tsdoc": "0.3.0",
    "globals": "15.9.0",
    "lint-staged": "15.2.10",
    "mocha": "10.7.3",
    "prettier": "3.3.3",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-sh": "0.14.0",
    "typescript": "5.6.2",
    "typescript-eslint": "8.7.0"
  },
  "engines": {
    "node": "20"
  },
  "packageManager": "yarn@4.5.0"
}
