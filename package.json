{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "yarn3-typescript-nodejs-esbuild",
  "license": "MIT",
  "author": "Oliver Salzburg <oliver.salzburg@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "yarn run build:tsc",
    "build:esbuild": "node build.js",
    "build:tsc": "tsc",
    "clean": "rm -rf ./output",
    "lint": "yarn run lint:all",
    "lint:all": "yarn run lint:eslint && yarn run lint:prettier && yarn run lint:tsc",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "test": "yarn build && .scripts/test.sh"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "2.12.0",
    "@aws-lambda-powertools/metrics": "2.12.0",
    "@aws-lambda-powertools/tracer": "2.12.0",
    "@aws-sdk/client-secrets-manager": "3.716.0",
    "@middy/core": "5.5.1",
    "@middy/event-normalizer": "5.5.1",
    "@middy/http-cors": "5.5.1",
    "@middy/http-error-handler": "5.5.1",
    "@middy/http-event-normalizer": "5.5.1",
    "@middy/http-header-normalizer": "5.5.1",
    "@middy/http-json-body-parser": "5.5.1",
    "@middy/http-response-serializer": "5.5.1",
    "@middy/http-security-headers": "5.5.1",
    "@middy/secrets-manager": "5.5.1",
    "@middy/validator": "5.5.1",
    "@oliversalzburg/js-utils": "0.2.2",
    "@types/json-schema": "7.0.15",
    "aws-lambda": "1.0.7",
    "json-schema-to-ts": "3.1.1"
  },
  "devDependencies": {
    "@eslint/js": "9.17.0",
    "@oliversalzburg/eslint-config": "1.2.0",
    "@types/aws-lambda": "8.10.146",
    "@types/chai": "5.0.1",
    "@types/mocha": "10.0.10",
    "@types/node": "22.10.2",
    "chai": "5.1.2",
    "esbuild": "0.24.1",
    "eslint": "9.17.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "50.6.1",
    "eslint-plugin-tsdoc": "0.4.0",
    "globals": "15.14.0",
    "lint-staged": "15.2.11",
    "mocha": "11.0.1",
    "prettier": "3.4.2",
    "prettier-package-json": "2.8.0",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-sh": "0.14.0",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.1"
  },
  "engines": {
    "node": "22"
  },
  "packageManager": "yarn@4.5.3"
}
